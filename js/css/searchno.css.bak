  }
}





@media (min-width: 1051px) and (max-width:1300px) {

  /* 탑바 */
  .top-wrap>.top-box1 {
    width: 95%;
  }
  
  .top-wrap>.top-box2>ul>li {
    padding: 23px 0px;
  
  }

}


@media (min-width: 851px) and (max-width:1050px) {

  /* 탑바 */
  .top-wrap>.top-box1 {
    width: 95%;
  }

  .top-wrap>.top-box2>ul>li {
    padding: 20px 0px;
  } 


}


@media (min-width: 500px) and (max-width:850px) {

  /* 탑바 */
  .top-wrap>.top-box1 {
    width: 95%;
  }

  .top-wrap>.top-box2 {
    font-size: 16px;
  }

  .top-wrap>.top-box2>ul>li {
    padding: 22px 0px;
  } 


}



