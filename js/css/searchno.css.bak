}

.top-wrap>.top-box1>.logo-box>.img-box>a>img {
  width: 150px;
}



.top-wrap>.top-box1>.search_wrap>.search-box>.schBtn>input[type=image] {
  width: 30px;
  height: 30px;
}







.top-wrap>.top-box1>.search_wrap>.search-box .form {
  width: 0;
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-bottom: 1px solid transparent;
  transition: width .3s, border-bottom-color .3s;
  background-color: transparent;
  color: #707070;
  margin-right: 10px;
  padding: 10px;
  display: block;
  box-sizing: border-box;
}

.top-wrap>.top-box1>.search_wrap>.search-box:hover .form {
  width: 170px;
  border-bottom-color: #707070;
}














.top-wrap>.top-box2 {
  margin: 0px auto;
  width: 100%;
  font-size: 18px;
  color: #707070;
  background: white;
  z-index: 100;
  font-family: 'ChosunSg';
  text-align: center;
  height: 60px;
}

.top-wrap>.top-box2>ul {
  width: 80%;
  margin: 0px auto;
}

.top-wrap>.top-box2>ul>li {
  width: 256px;
  padding: 20px 0px;
  position: relative;
  transition: all .4s;

}

.top-wrap>.top-box2>ul>li:hover {
  background: #ffeded;
}

.top-wrap>.top-box2>ul>li>div {
  visibility: hidden;
  position: absolute;
  z-index: 10;
}

/* .top-wrap > .top-box2 > ul>li:hover> div */
.top-wrap>.top-box2>ul>li:hover>div {
  visibility: visible;
  position: absolute;
  left: 0%;
  margin-top: 20px;
  background: white;
  width: 100%;
  transition: all .4s;
}

/* .top-wrap > .top-box2 > ul>li:hover> div > ul > li */
.top-wrap>.top-box2>ul>li:hover>div>ul>li {
  width: 100%;
  padding: 20px 0;
  text-align: center;
  transition: all .4s;
}

.top-wrap>.top-box2>ul>li:hover>div>ul>li:hover {
  background: #ffeded;
}


.top-wrap>.top-box2>ul>li>div>ul>li:last-child::after {
  border: none;
}



.search_wrap1 {
  width: 100%;
  margin-bottom: 100px;
}


.search_box {
  width: 60%;
  margin: 0 auto;
  font-size: 18px;
  box-sizing: border-box;
  border: 1px solid #c0c0c0;
  margin-bottom: 30px;
}

.search_box .search1 {
  padding: 20px 50px;
}

.search_box > .search1 > h2 {
  font-size: 25px;
}


.search_box > .search2 {
  display: flex;
  justify-content: space-between;
  background: #f6f6f6;
  padding: 15px 50px;
  border-top: 1px solid #c0c0c0;
}

.search_box > .search2 > .price {
  display: flex;
  align-items: center;
  font-size: 23px;
}

.search_box > .search2 > .price > span {
  color: #ff5151;
  font-size: 25px;
  align-items: center;
  margin: 3px;
  font-weight: bold;
}

.search_box > .search2 > a {
  border: 1px solid #ff5151;
  color: #ff5151;
  padding: 10px 15px;
  background: white;
  font-weight: bold;
  border-radius: 5px;
}














@media (min-width: 1301px) and (max-width:1920px) {

  /* 탑바 */
  .top-wrap>.top-box1 {
